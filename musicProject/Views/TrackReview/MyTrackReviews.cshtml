@model IEnumerable<musicProject.Models.TrackReviewModels.TrackReviewListItem>

<h1>My Track Reviews</h1>

<a asp-action="Create" asp-controller="TrackReview" class="text-primary">Review A Track</a>

<table class="table table-striped table-dark">
    <thead>
        <tr>
            <th>Track </th>
            <th>Rating</th>
            <th>Content</th>
            <th>&nbsp;</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var track in Model)
        {
            <tr>
                <td>@track.TrackTitle</td>
                <td>@track.Rating</td>
                <td><a asp-controller="TrackReview" asp-action="Detail" asp-route-id="@track.Id">Details</a> </td>
                <td><a asp-controller="TrackReview" asp-action="Update" asp-route-id="@track.Id">Edit</a> </td>
                <td><a asp-controller="TrackReview" asp-action="Delete" asp-route-id="@track.Id">Delete</a> </td>
            </tr>
        }
    </tbody>
</table>

<div>
    <a asp-action="NewIndex">Back to List</a>
</div>